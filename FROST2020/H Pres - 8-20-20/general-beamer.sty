%custom lists

\newcounter{FlatListNumber}
\newenvironment{FlatList}[1][1ex]  %no indentation
    {\begin{list}{Step \arabic{FlatListNumber}:}
      {\setlength{\itemsep}{#1 plus 0.2ex minus 0ex}
       \setlength{\topsep}{1ex plus 0.2ex minus 0ex}
       \setlength{\labelwidth}{-1ex}
       \setlength{\leftmargin}{0ex}
       \setlength{\itemindent}{5ex}
       \usecounter{FlatListNumber}}}
    {\end{list}}

\newcounter{CommentNumber}
\newenvironment{Comments}[1][2ex]
    {\begin{list}{(\arabic{CommentNumber})}
    {\setlength{\itemsep}{#1 plus 0.2ex minus 0ex}
         \setlength{\topsep}{1.5ex plus 0.2ex minus 0ex}
         %\setlength{\bottomsep}{1.5ex plus 0.2ex minus 0ex}
     \usecounter{CommentNumber}}}
    {\end{list}}
\newenvironment{ReComments}
    {\begin{list}{(\arabic{CommentNumber}')}
    {\setlength{\itemsep}{3ex plus 0.2ex minus 0ex}
         \setlength{\topsep}{1.5ex plus 0.2ex minus 0ex}
         %\setlength{\bottomsep}{1.5ex plus 0.2ex minus 0ex}
     \usecounter{CommentNumber}}}
    {\end{list}}

\newcounter{ProblemNumber}
\newenvironment{Prob}[1][4ex]
    {\begin{list}{\arabic{ProblemNumber}.}
    {\setlength{\itemsep}{#1 plus 0.2ex minus 0ex}
     \usecounter{ProblemNumber}}}
    {\end{list}}

\newcounter{ExampleNumber}
\newenvironment{Exmp}[1][4ex]
    {\begin{list}{Example (\arabic{ExampleNumber})}
    {\setlength{\itemsep}{#1 plus 0.2ex minus 0ex}
     \usecounter{ExampleNumber}}}
    {\end{list}}

\newcounter{SubProblemNumber}[ProblemNumber]
\newenvironment{Sub}[1][3ex]
    {\begin{list}{(\alph{SubProblemNumber})}
        {\setlength{\itemsep}{#1 plus 0.2ex minus 0ex}
         \setlength{\topsep}{1ex plus 0.2ex minus 0ex}
         \usecounter{SubProblemNumber}}}
    {\end{list}}

\newcounter{ConditionNumber}[SubProblemNumber]
\newenvironment{Cond}[1][1ex]
    {\begin{list}{(\roman{ConditionNumber})}
                {\setlength{\itemsep}{#1 plus 0.2ex minus 0ex}
                 \setlength{\topsep}{1ex plus 0.2ex minus 0ex}
                 \usecounter{ConditionNumber}}}
    {\end{list}}

\newcounter{CaseNumber}
\newenvironment{Case}[1][1ex]
    {\begin{list}{\text{case } (\arabic{CaseNumber})}
                {\setlength{\itemsep}{#1 plus 0.2ex minus 0ex}
                 \setlength{\topsep}{1ex plus 0.2ex minus 0ex}
                 \usecounter{CaseNumber}}}
    {\end{list}}

\newenvironment{BlankList}[1][1ex]
    {\begin{list}{}
                {\setlength{\itemsep}{#1 plus 0.2ex minus 0ex}
                 \setlength{\topsep}{1ex plus 0.2ex minus 0ex}
                 }}
    {\end{list}}

