\mode<presentation>

\definecolor{darkred}{rgb}{0.8,0,0}
\definecolor{CPgreen}{RGB}{3, 86, 66}
\definecolor{CPgold}{RGB}{180, 167, 108}

\setbeamercolor{section in toc}{fg=black,bg=white}
\setbeamercolor{alerted text}{fg=CPgreen!80!gray}
\setbeamercolor*{palette primary}{fg=CPgreen!60!black,bg=CPgold!30!white}
\setbeamercolor*{palette secondary}{fg=CPgreen!70!black,bg=CPgold!15!white}
\setbeamercolor*{palette tertiary}{bg=CPgreen!80!black,fg=CPgold!10!white}
\setbeamercolor*{palette quaternary}{fg=CPgreen,bg=CPgold!5!white}

\setbeamercolor*{sidebar}{fg=CPgreen,bg=CPgold!15!white}

\setbeamercolor*{palette sidebar primary}{fg=CPgreen!10!black}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{fg=CPgreen!50!black}
\setbeamercolor*{palette sidebar quaternary}{fg=CPgold!10!white}

%\setbeamercolor*{titlelike}{parent=palette primary}
\setbeamercolor{titlelike}{parent=palette primary,fg=CPgreen,bg=CPgold!10!white}
\setbeamercolor{frametitle}{bg=CPgold!10!white}
\setbeamercolor{frametitle right}{bg=gray!60!white}

\setbeamercolor*{separation line}{fg=CPgreen,bg=CPgreen}
\setbeamercolor*{fine separation line}{fg=CPgreen,bg=CPgreen}

%\setbeamertemplate{enumerate items}[circle]
\setbeamercolor{item}{bg=CPgreen,fg=white}
\setbeamercolor{item projected}{bg=CPgreen!50!CPgold,fg=white}

\usefonttheme[onlysmall]{structurebold}

\mode
<all>


